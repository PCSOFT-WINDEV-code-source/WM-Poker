  -
    name : BTN_CHANGER
    identifier : 0x2550dc460e6671ea
    internal_properties : 
    properties :
     x : 410
     y : 262
     width : 127
     height : 45
    type : 4
    layout :
     -
       x : 410
       y : 262
     -
