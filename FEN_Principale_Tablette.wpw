  procedures :
   -
     name : MiseJeu
     procedure_id : 1535969666166256106
     type_code : 14
     code : |1-
      PROCEDURE MiseJeu(_Val)
      
      // Soustrait val € au reste
      SAI_Reste -= _Val
      
      SI SAI_Reste < 0 ALORS 
      	//Plus de sous
      	Info(<§$0000§>)
      	SAI_Reste += _Val
      	RENVOYER Faux
      SINON
      	//Ajoute 1 € a la mise
      	SAI_Mise += _Val
      FIN
      
      RENVOYER Vrai
     type : 458752
   -
     name : Initialise
     procedure_id : 1562279179802936027
     type_code : 14
     code : |1+
      PROCEDURE Initialise()
      
      SAI_RESTE = cnMontantCréditInitial
      SAI_GAIN = 0
      SAI_MISE = 0
      LIB_EXPLICATION = <§$000f§>
      
      // Grise le bouton
      BTN_JOUER..Etat = Grisé
      BTN_TOUTCHANGER..Etat = Grisé
      BTN_Changer..Etat = Grisé
      GR_MISE..Etat = Actif
      
      // On remet toutes les cartes en place
      SI gclPoker:RetourneToutes() = Vrai ALORS
      	
      	ToastAffiche("Partie réinitialisée")
      FIN
      
