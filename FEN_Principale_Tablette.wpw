       y : 478
     -
       x : 410
       y : 250
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1+
         //Multiplicateur 
         eCoef est un entier
         eGain est un entier
         
         //Change les cartes
         gclPoker:Change()
         
         //Grise le bouton
         MoiMême..Etat = Grisé
         BTN_TOUTCHANGER..Etat = Grisé
         GR_MISE..Etat = Actif
         
         //Analyse du résultat
         eCoef = gclPoker:Analyse()
         
         //Selon de coefficient, on affiche ce qui a été gagné :
         SELON eCoef 
         	CAS 0 : LIB_EXPLICATION..Libellé = <§$000d§> 
         	CAS 1 : LIB_EXPLICATION..Libellé = <§$0003§> 
         	CAS 2 : LIB_EXPLICATION..Libellé = <§$0004§> 
         	CAS 3 : LIB_EXPLICATION..Libellé = <§$0005§>
         	CAS 4 : LIB_EXPLICATION..Libellé = <§$000e§>  
         	CAS 5 : LIB_EXPLICATION..Libellé = <§$0006§> 
         	CAS 6 : LIB_EXPLICATION..Libellé = <§$0007§> 
         	CAS 7 : LIB_EXPLICATION..Libellé = <§$0008§> 
         	CAS 8 : LIB_EXPLICATION..Libellé = <§$0009§> 
         	CAS 10 : LIB_EXPLICATION..Libellé = <§$000a§>
         FIN
         
         //On multiplie la mise par coefficient et on l'ajoute a RESTE
         eGain =  (SAI_Mise*eCoef)
         SAI_Reste = SAI_Reste + eGain
         SAI_Mise = 0
         
         SAI_Gain += eGain
         
         LIB_EXPLICATION..Libellé = ChaîneConstruit(LIB_EXPLICATION..Libellé + <§$000b§>,eGain)
         
         //Fin du Jeu
         SI SAI_Reste = 0 ALORS
         	SI OuiNon(Oui,<§$000c§>) ALORS
         		Initialise()
         	FIN
         	
         	// Soustrait 1 € au reste
         	SI MiseJeu(1) = vrai alors
         		// Libère le jeu
         		BTN_JOUER..Etat = Actif	
         	FIN
         	
         FIN
         
        type : 18
      -
        type : 161
        enabled : false
